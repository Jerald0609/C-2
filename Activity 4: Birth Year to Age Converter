using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        List<string> years = new();
        int currentYear = DateTime.Now.Year;

        for (int i = 0; i < 5; i++)
        {
            string input;
            int birthYear;
            do
            {
                Console.Write($"Enter birth year {i + 1}: ");
                input = Console.ReadLine();
            } while (!int.TryParse(input, out birthYear));

            years.Add(input);
            int age = currentYear - birthYear;

            string status = age < 18 ? "Minor" :
                            age < 60 ? "Adult" : "Senior";

            Console.WriteLine($"Age: {age} - {status}");
        }
    }
}
